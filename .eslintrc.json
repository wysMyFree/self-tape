{
    "env": {
      "node": true,
      "browser": false,
      "es6": true
    },
    "globals": {
      "fetch": false,
      "window": true,
      "navigator": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": ["error", {
        "devDependencies": true
      }],
      "no-underscore-dangle": ["error", {
        "allow": ["__REDUX_DEVTOOLS_EXTENSION__", "_value"]
      }]
    }
  }
  